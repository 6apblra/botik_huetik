# Токен Telegram-бота от @BotFather
BOT_TOKEN=your_telegram_bot_token_here

# API-ключ PayPalych (pally.info)
PAYPALYCH_API_KEY=your_paypalych_api_key_here

# URL базы данных (SQLite или PostgreSQL)
DATABASE_URL=sqlite:///vpn.db

# IP-адрес VPN-сервера
VPN_SERVER_IP=your_server_ip_here

# ID администратора (ваш Telegram ID)
ADMIN_ID=your_telegram_user_id_here

# Порт для v2ray (по умолчанию 443)
V2RAY_PORT=443

# Путь к конфигурации v2ray
V2RAY_CONFIG_PATH=/etc/v2ray/config.json
